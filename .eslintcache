e/user/WebstormProjects/imreact/im/src/components/Search.js",[],"/home/user/WebstormProjects/imreact/im/src/pages/ProductDetail.js",[],"/home/user/WebstormProjects/imreact/im/src/pages/Checkout.js",[],"/home/user/WebstormProjects/imreact/im/src/pages/CategoryDetail.js",["118","119"],"import React, {useContext, useEffect} from \"react\";\nimport axios from \"axios\";\nimport {Link} from \"react-router-dom\";\nimport {Sidebar} from \"../components/Sidebar\";\nimport {CatsContext} from \"../context/cats/catsContext\";\n\n// вывод списка товаров определенной категории\nexport const CategoryDetail = ( {match} ) => {\n    const {fetchProducts, fetchCats, cats, products} = useContext(CatsContext)\n    const id = match.params.id\n\n    useEffect(() => {\n        fetchProducts(id)\n        // eslint-disable-next-line\n    }, [id])\n\n    useEffect(() => {\n        fetchCats()\n        // eslint-disable-next-line\n    }, [])\n\n    return(\n            <div className=\"row\">\n                <div className=\"col-lg-3 mt-5\">\n                    <Sidebar/>\n                </div>\n                <div className=\"col-lg-9\">\n                    <nav aria-label=\"breadcrumb\" className=\"mt-5\">\n                        <ol className=\"breadcrumb\">\n                            <li className=\"breadcrumb-item\">\n                                <Link exact to='/'>Главная</Link>\n                            </li>\n                            <li className=\"breadcrumb-item active\">\n                                {cats.name}\n                            </li>\n                        </ol>\n                    </nav>\n\n                    <div className=\"row\">\n                        {products.map(p => (\n                            <div className=\"col-lg-4 col-md-6 mb-4\" key={p.id}>\n                                <div className=\"card h-100\">\n                                    <Link exact to={{pathname: `/product/${p.id}`, fromDashboard: false }}>\n                                        <img className=\"card-img-top\" src={`http://127.0.0.1:8000` + p.image} height=\"180\" width=\"245\"/>\n                                        Детали\n                                    </Link>\n                                    <div className=\"card-body\">\n                                        <h4 className=\"card-title\">\n                                            <Link exact to={{pathname: `/product/${p.id}`, fromDashboard: false}}>\n                                                {p.title}\n                                            </Link>\n                                        </h4>\n                                        <h5>p.price руб.</h5>\n                                        <Link exact to={{pathname: `/cart`, fromDashboard: false}}>\n                                            <button className=\"btn btn-danger\">Добавить в корзину(написать обработчик)</button>\n                                        </Link>\n                                    </div>\n                                </div>\n                            </div>\n                        ))}\n                    </div>\n                    </div>\n                </div>\n)\n}\n","/home/user/WebstormProjects/imreact/im/src/components/ProductsForMainPage.js",[],"/home/user/WebstormProjects/imreact/im/src/context/types.js",[],"/home/user/WebstormProjects/imreact/im/src/context/cart/cartContext.js",[],"/home/user/WebstormProjects/imreact/im/src/components/loader.js",[],"/home/user/WebstormProjects/imreact/im/src/context/cart/cartState.js",[],"/home/user/WebstormProjects/imreact/im/src/context/cart/cartReducer.js",[],"/home/user/WebstormProjects/imreact/im/src/context/cats/catsState.js",["120"],"import React, {useContext, useReducer} from 'react'\nimport axios from 'axios'\nimport {FETCH_PRODUCTS, FETCH_CATS, SHOW_LOADER} from '../types'\nimport {CatsContext} from \"./catsContext\";\nimport {catsReducer} from \"./catsReducer\";\n\n\nexport const CatsState = ({children}) => {\n\n    const initialState = {\n        cats: [],\n        products: [],\n        loading: false,\n    }\n    const [state, dispatch] = useReducer(catsReducer, initialState)\n\n    const showLoader = () => dispatch({type: SHOW_LOADER})\n\n    const fetchCats = async () => {\n        showLoader()\n            const res = await axios.get(`http://127.0.0.1:8000/api/category/`)\n            const payload = Object.keys(res.data).map(key => {\n                return {\n                    ...res.data[key],\n                }\n            })\n            dispatch({type: FETCH_CATS, payload})\n    }\n\n    const fetchProducts = async (id) => {\n        showLoader()\n            const res = await axios.get(`http://127.0.0.1:8000/api/category/${id}`)\n\n            const payload = Object.keys(res.data.products).map(key => {\n                return {\n                    ...res.data.products[key],\n                }\n            })\n            dispatch({type: FETCH_PRODUCTS, payload})\n    }\n\n    return (\n        <CatsContext.Provider value={{\n            showLoader, fetchProducts, fetchCats,\n            loading: state.loading,\n            products: state.products,\n            cats: state.cats\n        }}>\n            {children}\n        </CatsContext.Provider>\n    )\n}\n","/home/user/WebstormProjects/imreact/im/src/context/cats/catsContext.js",[],"/home/user/WebstormProjects/imreact/im/src/context/cats/catsReducer.js",[],"/home/user/WebstormProjects/imreact/im/src/components/Signup.js",[],"/home/user/WebstormProjects/imreact/im/src/components/Login.js",[],"/home/user/WebstormProjects/imreact/im/src/context/auth/authState.js",[],"/home/user/WebstormProjects/imreact/im/src/context/auth/authContext.js",[],"/home/user/WebstormProjects/imreact/im/src/context/auth/authReducer.js",[],{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","severity":1,"message":"126","line":1,"column":16,"nodeType":"127","messageId":"